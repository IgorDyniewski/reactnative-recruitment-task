{
  "name": "react-native-ui-kit-recruitment-task",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "yarn build:tailwind-css && yarn build:tailwind-json && echo Remember to run yarn dev:tailwind on a side to compile styles! && expo start",
    "start:prod": "yarn build:tailwind-css && yarn build:tailwind-json && expo start --no-dev --minify",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "build:tailwind-css": "tailwindcss -c ./tailwind/tailwind.config.js --input ./tailwind/input.css --output ./tailwind/output/styles.css --no-autoprefixer",
    "build:tailwind-json": "tailwind-rn --input ./tailwind/output/styles.css --output ./tailwind/output/styles.json",
    "dev:tailwind": "concurrently \"tailwindcss -c ./tailwind/tailwind.config.js --input ./tailwind/input.css --output ./tailwind/output/styles.css --no-autoprefixer --watch\" \"tailwind-rn --input ./tailwind/output/styles.css --output ./tailwind/output/styles.json --watch\"",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@expo-google-fonts/roboto": "^0.2.2",
    "@expo-google-fonts/work-sans": "^0.2.2",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/native-stack": "^6.6.2",
    "classnames": "^2.3.1",
    "expo": "~45.0.0",
    "expo-asset": "~8.5.0",
    "expo-font": "~10.1.0",
    "expo-localization": "~13.0.0",
    "expo-random": "~12.2.0",
    "expo-splash-screen": "~0.15.1",
    "expo-status-bar": "~1.3.0",
    "expo-web-browser": "~10.2.0",
    "humps": "^2.0.1",
    "i18n-js": "^3.9.2",
    "moment": "^2.29.3",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "prop-types": "^15.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.2",
    "react-native-safe-area-context": "4.2.4",
    "react-native-screens": "~3.11.1",
    "react-native-svg": "12.3.0",
    "react-native-web": "0.17.7",
    "react-redux": "^8.0.1",
    "redux": "^4.2.0",
    "styled-components": "^5.3.5",
    "tailwind-rn": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@types/classnames": "^2.3.1",
    "@types/humps": "^2.0.1",
    "@types/i18n-js": "^3.8.2",
    "@types/moment": "^2.13.0",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.66.13",
    "@types/react-redux": "^7.1.24",
    "@types/styled-components": "^5.1.25",
    "@types/styled-components-react-native": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "concurrently": "^7.2.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "postcss": "^8.4.13",
    "prettier": "^2.6.2",
    "tailwindcss": "^3.0.24",
    "typescript": "~4.3.5"
  },
  "resolutions": {
    "@types/react": "~17.0.21",
    "styled-components": "^5.3.5"
  },
  "private": true,
  "sourceType": "module"
}
